issn = {03784754},
journal = {Mathematics and Computers in Simulation},
keywords = {Non-conforming mixed FEM,Preconditioning},
number = {1-3},
pages = {149--154},
title = {{On a MIC (0) preconditioning of non-conforming mixed FEM elliptic problems}},
volume = {76},
year = {2007}
}
